@model IEnumerable<NOR_24.Models.RegistoUtilizador>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Regime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Valido)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Regime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Valido)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.RegistoId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.RegistoId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.RegistoId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
